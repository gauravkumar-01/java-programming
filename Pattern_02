import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	int row = 1;
	int star =1;
	while(row <= n)
	{
	    star = 1;
	    while(star <= row)
	    {
	        System.out.print("* ");
	        star++;
	    }
	    System.out.println();
	    row++;
	}
}
}
