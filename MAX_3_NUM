import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    Scanner sp = new Scanner(System.in);
    int b = sp.nextInt();
    Scanner sa = new Scanner(System.in);
    int c = sa.nextInt();
    if(a > b && a > c)
    {
        System.out.println(a);
    }
    else if(b > a && b > c)
    {
        System.out.println(b);
    }
    else
    {
        System.out.println(c);
    }
    
  }
}
